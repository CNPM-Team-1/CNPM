<AnchorPane fx:id="Host" prefHeight="720.0" prefWidth="1280.0" styleClass="navigation-body-border" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainNavigatorController">
